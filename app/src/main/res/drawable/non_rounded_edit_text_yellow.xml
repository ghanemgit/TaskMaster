<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:itemxmlns="http://schemas.android.com/aapt">

    <item>
        <shape
            android:padding="10dp"
            android:shape="rectangle">
            <solid android:color="#FBCB0A" />

        </shape>
    </item>

    <item
        android:left="-2dp"
        android:right="-2dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#001E6C" />
        </shape>
    </item>
</layer-list>